# Database Configuration
DATABASE_URL=bank.db

# JWT Secret for session tokens
JWT_SECRET=your-secure-jwt-secret-here-min-32-chars

# HMAC Key for SSN hashing (uniqueness checks without storing plaintext)
# Generate a random key: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# REQUIRED for production
HMAC_KEY=your-random-hex-key-here-32-bytes-recommended
